    <children>
        <Label layoutX="60.0" layoutY="118.0" prefHeight="26.0" prefWidth="94.0" text="Username: " />
        <TextField layoutX="62.0" layoutY="150.0" />
        
        <Label layoutX="60.0" layoutY="187.0" prefHeight="26.0" prefWidth="94.0" text="Password: " />
        <TextField layoutX="62.0" layoutY="217.0" />
        
        

        <Button fx:id="button" layoutX="62.0" layoutY="260.0" onAction="#handleLogin" text="Login" />

        <Button layoutX="414.0" layoutY="53.0" mnemonicParsing="false" onAction="#handleNewUser" text="Create New User" />
    </children>
</AnchorPane>
